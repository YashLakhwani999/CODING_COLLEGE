    "def fact(n):\n",
    "    if n == 0:\n",
    "        return 1\n",
    "    else:\n",
    "        return n * fact(n-1)\n",
    "n = int(input(\"Enter a number whose factorial you want : \"))\n",
    "print(fact(n))\n"
   ]
