   "source": [
    "def fibonacci_number(n):\n",
    "    if n == 1 or n == 2:\n",
    "        return 1\n",
    "    else:\n",
    "        return fibonacci_number(n-1) + fibonacci_number(n-2)\n",
    "n = int(input(\"Enter a number : \"))\n",
    "print(fibonacci_number(n))"
   ]
