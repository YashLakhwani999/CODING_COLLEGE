    "def sbs(s, ans):\n",
    "    if len(s) == 0:\n",
    "        print(ans)\n",
    "        return\n",
    "    \n",
    "    sbs(s[1:], ans)\n",
    "    newAns = ans + s[0]\n",
    "    sbs(s[1:], newAns)"
   ]
