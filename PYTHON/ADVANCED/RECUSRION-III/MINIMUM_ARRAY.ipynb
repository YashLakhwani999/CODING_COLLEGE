   "source": [
    "def minList(l, ans = 10000000):\n",
    "    if len(l) == 0:\n",
    "        print(ans)\n",
    "        return\n",
    "    \n",
    "    newMin = min(l[0], ans)\n",
    "    minList(l[1:], newMin)"
   ]
