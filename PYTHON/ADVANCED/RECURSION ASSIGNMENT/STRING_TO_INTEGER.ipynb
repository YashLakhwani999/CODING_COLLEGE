   "source": [
    "def string_2_int(a):\n",
    "    if len(a) == 1:\n",
    "        return int(a)\n",
    "    else:\n",
    "        return int(a[0]) * (10**(len(a)-1)) + string_2_int(a[1:])"
   ]
  },
