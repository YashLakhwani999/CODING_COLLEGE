   "source": [
    "def Stair_case(n):\n",
    "    if ( n == 0 ):\n",
    "        return 1\n",
    "    elif (n < 0):\n",
    "        return 0\n",
    " \n",
    "    else:\n",
    "        return Stair_case(n - 3) + Stair_case(n - 2) + Stair_case(n - 1)"
   ]
  },
