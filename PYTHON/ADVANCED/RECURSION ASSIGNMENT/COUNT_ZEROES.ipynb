   "source": [
    "import sys\n",
    "sys.setrecursionlimit(3000000)\n",
    "def count_0(n):\n",
    "    if n == 0:\n",
    "        return 0\n",
    "    \n",
    "    if n % 10 == 0:\n",
    "        return 1 + count_0(n//10)\n",
    "    else:\n",
    "        return count_0(n//10)"
   ]
  },
